{{#each this}}
<div class="daily-box" id=daily-{{@index}}>
  <div class="daily__date"></div>
  <div class="daily__weather"><img src={{weather.icon}} alt={{weather.main}} /></div>
  <div class="daily__sunrise"><i class="wi wi-sunrise"></i><span>{{sunrise}}</span></div>
  <div class="daily__sunset"><i class="wi wi-sunset"></i><span>{{sunset}}</span></div>
  <div class="daily__temperature">
    <div class="dayt"><i class="wi wi-thermometer"></i><span>{{temp}}</span></div>
  </div>
  <div class="daily__feelslike">
    <div class="fday">Feels Like<span>{{feels_like}}</span></div>
  </div>
  <div class="daily__pressure"><i class="wi wi-barometer"></i><span>{{pressure}}</span></div>
  <div class="daily__humidity"><i class="wi wi-humidity"></i><span>{{humidity}}</span></div>
  <div class="daily__dew-point"><i class="wi wi-raindrop"></i><span>{{dew_point}}</span></div>
  <div class="daily__windspeed"><i class="wi wi-gale-warning"></i> <span>{{wind_speed}}</span></div>
  <div class="daily__wind-dir"><i class="wi wi-wind towards-{{wind_deg}}-deg"></i><span>{{wind_deg}}Ëš</span></div>
</div>
{{/each}}